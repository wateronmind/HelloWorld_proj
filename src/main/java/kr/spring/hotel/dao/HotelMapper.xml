<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.hotel.dao.HotelMapper">

	<select id="selectList" resultType="hotelCommand">
		SELECT * FROM STAYING_INFO ORDER BY ST_ID
	</select>
	
	<select id="selectListRow" resultType="integer">
		SELECT COUNT(*) FROM STAYING_INFO
	</select>

</mapper>